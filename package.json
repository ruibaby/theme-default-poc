    "jsdom": "^20.0.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.17",
    "prettier": "^2.7.1",
    "prettier-plugin-tailwindcss": "^0.1.13",
    "sass": "^1.55.0",
    "tailwindcss": "^3.1.8",
    "typescript": "~4.7.4",
    "unplugin-icons": "^0.14.11",
    "vite": "^3.1.4",
    "vitest": "^0.19.1",
    "vue": "^3.2.40",
    "vue-tsc": "^0.39.5"
